<%= simple_form_for(@bib_section) do |f|  %>
  <%= f.error_notification %>


  <div class="form-inputs">
    <%= f.input :library_id, as: 'hidden' %>
    <%= f.input :name, as: 'string', input_html: {size: 80} %>
    <%= f.input :external %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, value: 'Salva' , class:'btn btn-success'%>
  </div>
<% end %>


