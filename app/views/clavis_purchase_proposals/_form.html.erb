


<%= simple_form_for(@clavis_purchase_proposal) do |f|  %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :proposal_id, as: 'hidden' %>

    <%= f.input :title, as: 'string', input_html: {size: 80} %>
    <%= f.input :ean, as: 'string', input_html: {size: 13} %>
    <%= f.input :librarian_notes, as: 'string', input_html: {size: 80} %>



  </div>

  <div class="form-actions">
    <%= f.button :submit, value: 'Salva' , class:'btn btn-success'%>
  </div>
<% end %>


