
<h4>Inserimento fornitore (che deve essere presente in Clavis)</h4>

<%= simple_form_for(@sbct_supplier) do |f|  %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :supplier_id, label:'Id fornitore in Clavis' %>

  </div>

  <div class="form-actions">
    <%= f.button :submit, value: 'Salva' , class:'btn btn-success'%>
  </div>
<% end %>



