<% render :partial=>'searchform' %>


<%= link_to('cerca nei metadati ', d_objects_path, class:'btn btn-info') %>
<%= link_to('area personale ', d_objects_folder_path(0), class:'btn btn-info') %>

<% content_tag(:pre, @sql) %>


<% if @do_search %>

<%= will_paginate @d_objects_folders, renderer: BootstrapPagination::Rails %>

<%= content_tag(:h3, "#{@d_objects_folders.total_entries} cartelle") %>

<%= content_tag(:div, d_objects_folders_list(@d_objects_folders)) %>

<% else %>

<%= content_tag(:div, d_objects_folders_root) %>


<% end %>

