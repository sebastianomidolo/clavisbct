<%= simple_form_for(@bio_iconografico_namespace) do |f|  %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :label, as:'hidden' %>
    <%= f.input :clavis_username, label:'username in Clavis' %>

  </div>

  <div class="form-actions">
    <%= f.button :submit, value: 'Salva' , class:'btn btn-success'%>
  </div>
<% end %>


