<% if !@cm.iccu_opac_url.nil? %>
jQuery('#items').after('<b><a href="<%= @cm.iccu_opac_url -%>" title="Vedi sul Catalogo in linea del Sistema Bibliotecario Nazionale" target="_new">Vedi altre biblioteche italiane che potrebbero avere questo libro, sul Catalogo SBN nazionale</a></b>');
<% end %>
