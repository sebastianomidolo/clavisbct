<% html=''
   html << container_items_list(@container_items)
   html << clavis_consistency_notes_periodici_in_casse(@cm.periodici_in_casse)
   html << content_tag(:h3, @form_richiesta.html_safe) if !@form_richiesta.blank?
   html=html.html_safe if !html.blank?
%>

<% if !html.blank? %>
 if (jQuery('#issues').length) {
  jQuery('.panel-title a',jQuery('#issues').parent()).text('Fascicoli (click per maggiori informazioni)');
  jQuery('#issues').after('<div><%= html -%></div>');
 } else {
  jQuery('#items').after('<div><%= html -%></div>');
 }
<% end %>

// alert('VEA0092905');
