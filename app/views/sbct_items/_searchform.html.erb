  
<%= simple_form_for(@sbct_item, :method=>'get', :html => {:class => 'form-vertical', role: 'form'} ) do |f|  %>
  <%= f.error_notification %>

  <div class="form-inputs">

         <%= f.association :clavis_library, collection: SbctTitle.libraries_select, hint: nil,
	 as:'select', include_blank:true, label: 'Biblioteca'   %>


    
	
    <% f.input :numcopie, label:'Numero copie' %>
    <%= f.association :sbct_supplier, label:'Fornitore' %>
    <%= f.association :sbct_budget, label:'Budget' %>
    <%= f.association :sbct_order_status, label:"Stato dell'ordine" %>

  </div>

  <div class="form-actions">
    <%= f.button :submit, value: 'Cerca' , class:'btn btn-success'%>
  </div>
<% end %>


